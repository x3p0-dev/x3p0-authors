{
	"name": "x3p0-authors",
	"description": "Displays a list of authors.",
	"author": "Justin Tadlock",
	"homepage": "https://github.com/x3p0-dev/x3p0-authors",
	"scripts": {
		"start": "npm run start:blocks && npm run manifest",
		"build": "npm run build:blocks && npm run manifest",
		"start:blocks": "wp-scripts start --webpack-src-dir=resources/blocks --output-path=public/blocks",
		"build:blocks": "wp-scripts build --webpack-src-dir=resources/blocks --output-path=public/blocks",
		"manifest": "wp-scripts build-blocks-manifest --input=resources --output=public/blocks/manifest.php",
		"packages-update": "wp-scripts packages-update"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.24.0"
	},
	"dependencies": {
		"@wordpress/block-editor": "^15.4.0",
		"@wordpress/blocks": "^15.4.0",
		"@wordpress/components": "^30.4.0",
		"@wordpress/core-data": "^7.31.0",
		"@wordpress/data": "^10.31.0",
		"@wordpress/i18n": "^6.4.0",
		"@wordpress/icons": "^10.31.0",
		"@wordpress/primitives": "^4.31.0"
	}
}
